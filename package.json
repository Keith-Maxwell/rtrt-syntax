{
    "name": "rtrt-syntax",
    "displayName": "RTRT syntax",
    "description": "Adds syntax highlighting for the rtscript language, used in .ptu files in RTRT",
    "version": "1.0.0",
    "author": "Max Perraudin",
    "publisher": "max-per",
    "engines": {
        "vscode": "^1.69.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Keith-Maxwell/rtrt-syntax"
    },
    "bugs": "https://github.com/Keith-Maxwell/rtrt-syntax/issues",
    "icon": "rtrt-syntax_icon_128px.png",
    "readme": "README.md",
    "license": "MIT",
    "categories": [
        "Programming Languages", "Snippets"
    ],
    "keywords": ["rtrt", "ptu", "rtscript", "syntax", "highlight"],
    "contributes": {
        "languages": [{
            "id": "rtscript",
            "aliases": ["rtscript", "ptu", "rtrt"],
            "extensions": [".ptu"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "rtscript",
            "scopeName": "source.rtscript",
            "path": "./syntaxes/rtscript.tmLanguage.json"
        }],
        "snippets": [
            {
                "language": "rtscript",
                "path": "./snippets/snippets.json"
            }
        ]
    }
}
